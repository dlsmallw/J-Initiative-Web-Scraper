{
  "source": {
    "include": ["src/backend", "src/electron", "src/frontend/components/views"],
    "exclude": ["src/tests", "src/frontend/assets"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./jsdocs",
    "recurse": true,
    "template": "templates/default"
  }
}
